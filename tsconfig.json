{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "typeRoots": ["./src/types", "./node_modules/@types"],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "src",
     "app",
      "context",
     "types", 
     "hooks", 
     "utils", 
     "theme", 
     "database",
     "components"
  ]
}